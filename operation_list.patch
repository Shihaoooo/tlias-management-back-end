+<?xml version="1.0" encoding="UTF-8" ?>
+<!DOCTYPE mapper
+        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
+        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
+<mapper namespace="com.itheima.mapper.EmpMapper">
+    <select id="queryEmp" resultType="com.itheima.pojo.Emp">
+        <!--id指mapper接口中映射的方法名；resultType指单行数据封装的对象类型-->
+        select e.name as username,e.gender,e.image,e.job,e.date,d.name as deptname,e.update_time from emp e left join dept d on e.dept_id = d.id
+        where e.name like concat('%',#{name},'%')
+        and gender = #{gender}
+        and date >= #{date}
+        order by e.update_time desc
+    </select>
+
+
+</mapper>
